            <info copy="1" />
            <location>
                <dimension name="weight" xvalue="400.000000" />
                <dimension name="width" xvalue="100.000000" />
                <dimension name="custom" xvalue="0.000000" />
            </location>
        </source>
        <source familyname="AnnapurnaSIL" filename="AnnapurnaSIL-Bold.ufo" name="AnnapurnaSIL Bold" stylename="Bold">
            <location>
                <dimension name="weight" xvalue="700" />
                <dimension name="width" xvalue="100" />
                <dimension name="custom" xvalue="0" />
            </location>
        </source>
    </sources>
    <instances>
